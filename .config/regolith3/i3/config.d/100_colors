# Load theme colors from Pywal
set_from_resource $fg i3wm.foreground #34eb3a
set_from_resource $bg i3wm.background #34eb3a

set_from_resource $color0 i3wm.color0 #34eb3a
set_from_resource $color1 i3wm.color1 #34eb3a
set_from_resource $color2 i3wm.color2 #34eb3a
set_from_resource $color3 i3wm.color3 #34eb3a
set_from_resource $color4 i3wm.color4 #34eb3a
set_from_resource $color5 i3wm.color5 #34eb3a
set_from_resource $color6 i3wm.color6 #34eb3a
set_from_resource $color7 i3wm.color7 #34eb3a
set_from_resource $color8 i3wm.color8 #34eb3a
set_from_resource $color9 i3wm.color9 #34eb3a
set_from_resource $color10 i3wm.color10 #34eb3a
set_from_resource $color11 i3wm.color11 #34eb3a
set_from_resource $color12 i3wm.color12 #34eb3a
set_from_resource $color13 i3wm.color13 #34eb3a
set_from_resource $color14 i3wm.color14 #34eb3a
set_from_resource $color15 i3wm.color15 #34eb3a

# Set client colors
client.focused            $color9 $color1 $color0 $color4 $color9
client.focused_inactive   $color0 $color0 $color7 $color0 $color0
client.unfocused          $color0 $color0 $color7 $color0 $color0
client.urgent             $color4 $color4 $color0 $color1 $color4
